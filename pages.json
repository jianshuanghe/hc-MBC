{
	"pages": [
		// pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/mbc/home",
			"style": {
				"navigationBarTitleText": "陌拜资本",
				"app-plus": {}
			}
		},
		{
			"path": "pages/mbc/template/landRegistration/landRegistration",
			"style": {
				"navigationBarTitleText": "登录注册",
				"app-plus": {}
			}
		},
		{
			"path": "pages/mbc/template/landRegistration/forgetPassWord",
			"style": {
				"navigationBarTitleText": "忘记密码",
				"app-plus": {}
			}
		}
	],
	"subPackages": [{
			"root": "modules/pageHome",
			"pages": [{
				"path": "homeList/homeList",
				"style": {
					"navigationBarTitleText": "首页",
					"navigationStyle": "custom",
					"enablePullDownRefresh": true,
					"app-plus": {
						"titleNView": false,
						"bounce": "none",
						"pullToRefresh": {
							"style": "circle",
							"offset": "70px"
						}
					}
				}
			}]
		},
		{
			"root": "modules/pageFind",
			"pages": [{
					"path": "findList/findList",
					"style": {
						"navigationBarTitleText": "发现",
						"enablePullDownRefresh": true,
						"onReachBottomDistance": 20
					}
				}
			]
		}, {
			"root": "modules/pagePublish",
			"pages": [{
					"path": "publishList/publishList",
					"style": {
						"navigationBarTitleText": "发布"
					}
				}
			]
		}, {
			"root": "modules/pageNews",
			"pages": [{
					"path": "newsList/newsList",
					"style": {
						"navigationBarTitleText": "消息"
					}
				}
			]
		}, {
			"root": "modules/pageMy",
			"pages": [{
					"path": "myList/myList",
					"style": {
						"navigationBarTitleText": "我的"
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "Hello uniapp",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"backgroundColorTop": "#F4F5F6",
		"backgroundColorBottom": "#F4F5F6",
		"mp-alipay": {
			"titleBarColor": "#FFFFFF"
		}
	}
}
